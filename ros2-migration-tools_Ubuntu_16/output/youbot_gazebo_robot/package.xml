<?xml version='1.0' encoding='utf-8'?>
<package format="3">
  <name>youbot_gazebo_robot</name>
  <version>0.1.0</version>
  <description>Launch the KUKA youBot in the Gazebo simulation</description>
  <author email="frederik.hegger@h-brs.de">Frederik Hegger</author>
  <maintainer email="nowak@locomotec.com">Walter Nowak</maintainer>
  <maintainer email="blumenthal@locomotec.com">Sebastian Blumenthal</maintainer>
  <license>GPLv3</license>
  <url type="website">http://github.com/mas-group/youbot_simulation</url>
  <buildtool_depend>ament_cmake</buildtool_depend>
  <exec_depend>gazebo_ros</exec_depend>
  <exec_depend>robot_state_publisher</exec_depend>
  <exec_depend>youbot_gazebo_control</exec_depend>
  <exec_depend>youbot_gazebo_worlds</exec_depend>
  <test_depend>roslaunch</test_depend>
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
